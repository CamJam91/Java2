package edu.sinclair.cameron_murphy;

import java.awt.Color;
import java.awt.Graphics;
import java.awt.Point;

public class House {
	public static void draw(Graphics g) {
		HouseBase.draw(g);
		Door.draw(g);
		Roof.draw(g);
		Window windowLeft = new Window(g, new Point(300, 650), Color.WHITE, 50, 50);
		Window windowRight = new Window(g, new Point(550,650), Color.WHITE, 50,50);
		Window windowTopLeft = new Window(g, new Point(300,530), Color.WHITE, 60, 80);
		Window windowTopRight = new Window(g, new Point(550,530), Color.WHITE, 60, 80);
		
	}
	
}
