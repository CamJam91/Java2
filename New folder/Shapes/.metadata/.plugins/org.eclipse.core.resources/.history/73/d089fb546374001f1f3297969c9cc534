package edu.sinclair.cameron_murphy;

import java.awt.Color;
import java.awt.Graphics;
import java.awt.Point;

public class Tree {
	private Point pointA;
	public Tree(Graphics g, Point pointA) {
		Box stump = new Box(pointA, new Color(74, 42, 1),50,75);
		stump.draw(g);
		Triangle test = new Triangle(new Point(200,200), new Point(250,300),new Color(0,255,0),50);
		test.draw(g);
	}
}
