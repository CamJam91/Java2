package edu.sinclair.cameron_murphy;

import java.awt.Color;
import java.awt.Graphics;
import java.awt.Point;

public class House {
	public static void draw(Graphics g) {
		HouseBase.draw(g);
		Door.draw(g);
		Roof.draw(g);
		Window windowLeft = new Window(g, new Point(300, 600), Color.WHITE, 50, 50);
		Window windowRight = new Window(g, new Point(800,600), Color.WHITE, 50,50);
	}
	
}
